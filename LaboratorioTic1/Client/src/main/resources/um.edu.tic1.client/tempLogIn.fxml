<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="500.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="um.edu.tic1.client.controllers.TempLogIn">
   <children>
      <TextField fx:id="userName" layoutX="38.0" layoutY="160.0" promptText="USER NAME" />
      <TextField fx:id="password" layoutX="38.0" layoutY="194.0" promptText="PASSWORLD" />
      <Label layoutX="123.0" layoutY="26.0" text="LOG IN">
         <font>
            <Font size="48.0" />
         </font>
      </Label>
      <Button fx:id="logIn" layoutX="38.0" layoutY="228.0" mnemonicParsing="false" onAction="#logIn" text="LOG IN" textFill="#da1e1e" />
      <RadioButton fx:id="client" layoutX="38.0" layoutY="105.0" mnemonicParsing="false" text="CLIENT">
         <toggleGroup>
            <ToggleGroup fx:id="toggleGroup" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="brand" layoutX="163.0" layoutY="105.0" mnemonicParsing="false" text="BRAND" toggleGroup="$toggleGroup" />
      <RadioButton fx:id="admin" layoutX="38.0" layoutY="132.0" mnemonicParsing="false" text="ADMINISTRADOR" toggleGroup="$toggleGroup" />
      <RadioButton fx:id="store" layoutX="165.0" layoutY="132.0" mnemonicParsing="false" text="STORE" toggleGroup="$toggleGroup" />
      <Button fx:id="check" layoutX="303.0" layoutY="461.0" mnemonicParsing="false" onAction="#checkUserIsLogIn" text="TEST LOG IN" />
      <Label fx:id="revisar" layoutX="248.0" layoutY="435.0" prefHeight="17.0" prefWidth="138.0" />
      <Label fx:id="mensaje" layoutX="38.0" layoutY="266.0" prefHeight="17.0" prefWidth="343.0" />
   </children>
</AnchorPane>
